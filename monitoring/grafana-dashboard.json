    {
      "id": 3,
      "title": "Error Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(rate(http_request_duration_ms_count{status_code!~\"2..\"}[1m])) / sum(rate(http_request_duration_ms_count[1m]))",
          "refId": "A",
          "legendFormat": "Error Rate"
        }
      ],
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 8
      },
      "fieldConfig": {
        "defaults": {
          "color": {
